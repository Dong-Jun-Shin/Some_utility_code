{
  "name": "github-analyzer",
  "version": "0.0.0",
  "description": "analyze github",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/bin/www.ts",
    "start:dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" src/bin/www.ts",
    "": "------------------------------------------------------------------",
    "legacy:build": "NODE_ENV=production babel src --out-dir dist --copy-files",
    "legacy:start": "node dist/bin/www.js",
    "legacy:start:dev": "nodemon --exec babel-node src/bin/www.js"
  },
  "author": "Dong-jun-Shin",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.7",
    "cookie-parser": "~1.4.4",
    "dayjs": "^1.11.10",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "duration": "^0.2.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jest": "^29.7.0",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11",
    "stringify-object": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/node": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
